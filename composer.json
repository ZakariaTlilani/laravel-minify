{
  "name": "zakariatlilani/laravel-minify",
  "description": "laravel minify html output",
  "keywords": [
    "zakariatlilani",
    "laravel",
    "minify",
    "compress"
  ],
  "homepage": "https://github.com/zakariatlilani/laravel-minify",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "zakaria tlilani",
      "email": "tlilanizakaria@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "zakariatlilani\\LaravelMinify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "zakariatlilani\\LaravelMinify\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "zakariatlilani\\LaravelMinify\\Providers\\LaravelMinifyServiceProvider",
        "zakariatlilani\\LaravelMinify\\Providers\\ExcludeMinifyBladeServiceProvider"
      ],
      "aliases": {
        "LaravelMinify": "zakariatlilani\\LaravelMinify\\LaravelMinifyFacade"
      }
    }
  }
}
